{
  "version": 2,
  "builds": [
    { "src": "content/**", "use": "@now/static" },
    { "src": "package.json", "use": "@now/next" }
  ],
  "routes": [
    { "src": ".*", "headers": { "Access-Control-Allow-Origin": "*" } },
    { "src": "/pages/(?<id>[^/]+)$", "dest": "/pages?id=$id&type=pages" },
    { "src": "/blog/(?<id>[^/]+)$", "dest": "/pages?id=$id&type=blog" }
  ],
  "alias": "dougpalm.com",
  "name": "dougpalm"
}
