{
  "name": "dougpalm.com",
  "description": "Personal website of Doug Palm",
  "author": "David Sinclair <david@sinclair.tech> (https://sinclair.tech)",
  "homepage": "https://dougpalm.com",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js"
  },
  "xo": {
    "parser": "babel-eslint",
    "prettier": true,
    "extends": "xo-react",
    "rules": {
      "react/no-danger": 0,
      "unicorn/filename-case": 0
    },
    "settings": {
      "import/core-modules": [
        "styled-jsx",
        "styled-jsx/css"
      ],
      "react": {
        "version": "16.7.0-alpha.2"
      }
    },
    "env": [
      "browser",
      "node"
    ],
    "space": 2
  },
  "dependencies": {
    "axios": "0.18.0",
    "he": "1.2.0",
    "lodash": "4.17.11",
    "moment": "2.23.0",
    "next": "11.1.3",
    "path-match": "1.2.4",
    "prop-types": "15.6.2",
    "raw-loader": "1.0.0",
    "react": "16.7.0-alpha.2",
    "react-dom": "16.7.0-alpha.2",
    "showdown": "1.9.0"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.12.1",
    "eslint-config-xo": "0.26.0",
    "eslint-config-xo-react": "0.18.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "prettier": "1.16.1",
    "xo": "0.24.0"
  }
}
